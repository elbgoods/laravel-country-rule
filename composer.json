{
  "name": "elbgoods/laravel-country-rule",
  "description": "Laravel validation rules for ISO-3166 country formats.",
  "keywords": [
    "laravel",
    "validation",
    "country",
    "iso3166"
  ],
  "homepage": "https://github.com/elbgoods/laravel-country-rule",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Witkowski",
      "email": "twitkowski@elbgoods.de",
      "homepage": "https://elbgoods.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "illuminate/contracts": "^6.0 || ^7.0 || ^8.0",
    "illuminate/support": "^6.0 || ^7.0 || ^8.0",
    "league/iso3166": "^2.1 || ^3.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0 || ^5.0 || ^6.0",
    "phpunit/phpunit": "^8.0 || ^9.0"
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Elbgoods\\CountryRule\\CountryRuleServiceProvider"
      ]
    }
  },
  "autoload": {
    "psr-4": {
      "Elbgoods\\CountryRule\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elbgoods\\CountryRule\\Tests\\": "tests"
    }
  }
}
