{
  "name": "elbgoods/laravel-country-rule",
  "description": "Laravel validation rules for ISO-3166 country formats.",
  "keywords": [
    "laravel",
    "validation",
    "country",
    "iso3166"
  ],
  "homepage": "https://github.com/elbgoods/laravel-country-rule",
  "license": "MIT",
  "authors": [
    {
      "name": "Tom Witkowski",
      "email": "dev.gummibeer@gmail.com",
      "homepage": "https://gummibeer.de",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^7.4",
    "illuminate/support": "^6.0",
    "league/iso3166": "^2.1"
  },
  "require-dev": {
    "codedungeon/phpunit-result-printer": "^0.26.2",
    "elbgoods/ci-test-tools": "^1.5.0",
    "nunomaduro/collision": "^3.0",
    "orchestra/testbench": "^4.0",
    "phpunit/phpunit": "^8.0"
  },
  "autoload": {
    "psr-4": {
      "Elbgoods\\CountryRule\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Elbgoods\\CountryRule\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "extra": {
    "laravel": {
      "providers": [
        "Elbgoods\\CountryRule\\CountryRuleServiceProvider"
      ]
    }
  }
}
